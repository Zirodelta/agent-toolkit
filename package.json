{
  "name": "zirodelta-agent-toolkit",
  "version": "0.1.0",
  "description": "Agent-native infrastructure for autonomous funding rate farming with Zirodelta",
  "type": "module",
  "main": "./dist/sdk/index.js",
  "types": "./dist/sdk/index.d.ts",
  "bin": {
    "zirodelta": "./bin/zirodelta.js"
  },
  "exports": {
    ".": {
      "types": "./dist/sdk/index.d.ts",
      "import": "./dist/sdk/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "SKILL.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "cli": "node --loader ts-node/esm src/cli/index.ts",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zirodelta",
    "funding-rate",
    "arbitrage",
    "defi",
    "agent",
    "trading",
    "crypto"
  ],
  "author": "Zirodelta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zirodelta/agent-toolkit.git"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
